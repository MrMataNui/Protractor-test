{
	"name": "travelocity-testing",
	"version": "0.0.0",
	"scripts": {
		"clean": "rm -rf ./node_modules",
		"reinstall": "npm run clean && npm install",
		"debug": "node --inspect-brk ./node_modules/.bin/protractor conf.js",
		"pretest": "npm run tsc && npm run webdriver-update",
		"test": "protractor conf.js",
		"tsc": "tsc",
		"webdriver-update": "webdriver-manager update --standalone=false --gecko=false"
	},
	"private": true,
	"dependencies": {
		"@types/chai": "^4.1.7",
		"@types/jasmine": "^3.3.12",
		"@types/node": "^12.6.8",
		"chai": "^4.2.0",
		"cucumber": "^5.1.0",
		"jasmine": "^3.3.1",
		"protractor": "^5.4.2",
		"protractor-cucumber-framework": "^6.1.3",
		"ts-node": "^8.0.3",
		"typescript": "^3.4.1"
	},
	"devDependencies": {
		"@types/selenium-webdriver": "^4.0.0"
	}
}
